  "scripts": {
    "build": "babel src --extensions \".js,.ts\" --out-dir build --copy-files",
    "dev": "ts-node-dev -r tsconfig-paths/register --inspect --transpile-only --exit-child --ignore-watch node_modules src/shared/infra/http/server.ts",
    "test": "jest",
    "test:e2e": "jest --config jest-e2e.config.ts",
    "lint-fix": "eslint . --ext .ts --fix",
    "migrate:latest": "prisma migrate dev",
    "migrate:rollback": "prisma migrate reset",
    "migrate:make": "prisma migrate dev --name",
    "migrate:make:only": "prisma migrate dev --create-only --name",
    "migrate:updb": "prisma db push",
    "prisma:seeds": "ts-node-dev prisma/seeds ",
    "prisma:studio": "prisma studio --port 5555"
  },
